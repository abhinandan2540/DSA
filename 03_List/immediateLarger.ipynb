   "source": [
    "# finding immediate larger element\n",
    "\n",
    "def immediateLarger(arr:list, x:int):\n",
    "    ans=-1\n",
    "    diff=1000000\n",
    "    for i in arr:\n",
    "        if(i>x and i-x<diff):\n",
    "            ans=i\n",
    "            diff=i-x\n",
    "    return ans\n",
    "\n",
    "\n",
    "print(immediateLarger([63, 6, 60, 38, 3, 94, 93, 83, 65], 9))"
   ]
  }
 ],
