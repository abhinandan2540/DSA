   "source": [
    "\n",
    "def immediateSmaller(arr:list, x:int):\n",
    "    ans=-1\n",
    "    diff=1000000\n",
    "    for i in arr:\n",
    "        if(i<x and x-i<diff):\n",
    "            ans=i\n",
    "            diff=x-i\n",
    "    return ans\n",
    "\n",
    "print(immediateSmaller([63,6,60,38,3,94,93,83,65],9))\n",
    "\n",
    "# time complexity : O(n)"
   ]
  }
